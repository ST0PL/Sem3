<Application x:Class="ILS_WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ILS_WPF"
             xmlns:cconv="clr-namespace:ControlsLib.Converters;assembly=ControlsLib"
             xmlns:conv="clr-namespace:ILS_WPF.Converters">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Resources/Converters.xaml"/>
                <ResourceDictionary Source="/Resources/Templates/SupplyRequestDetailTemplates.xaml"/>
                <ResourceDictionary Source="/Resources/Templates/WarehouseEntryTemplates.xaml"/>
                <ResourceDictionary Source="/Resources/Icons/XamlIcons.xaml"/>
                <ResourceDictionary Source="/ControlsLib;component/Styles/Generic.xaml"/>
                <ResourceDictionary Source="/Resources/Styles/Colors.xaml"/>
                <ResourceDictionary Source="/Resources/Styles/Buttons.xaml"/>
                <ResourceDictionary Source="/Resources/Styles/Effects.xaml"/>
                <ResourceDictionary Source="/Resources/Styles/ComboBoxes.xaml"/>
                <ResourceDictionary Source="/Resources/Styles/ListViews.xaml"/>
                <ResourceDictionary Source="/Resources/Strings.xaml"/>
                <ResourceDictionary Source="/Resources/Styles/TextBoxes.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <FontFamily x:Key="Inter">/ILS_WPF;component/Fonts/#Inter</FontFamily>
        </ResourceDictionary>
    </Application.Resources>
</Application>
